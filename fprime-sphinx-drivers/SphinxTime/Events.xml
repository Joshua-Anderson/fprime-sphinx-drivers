<events>
  <event id="0" name="SphinxTime_Update" severity="ACTIVITY_HI" format_string="SCLK Value updated. Old SCLK: %u.%06u, New SCLK: %u.%06u">
    <comment>
     Spacecraft clock (SCLK) value updated from old to new values 
    </comment>
    <args>
      <arg name="old_sclk_secs" type="U32">
        <comment>Old SCLK value whole seconds</comment>
      </arg>
      <arg name="old_sclk_usecs" type="U32">
        <comment>Old SCLK value sub-seconds</comment>
      </arg>
      <arg name="new_sclk_secs" type="U32">
        <comment>New SCLK value whole seconds</comment>
      </arg>
      <arg name="new_sclk_usecs" type="U32">
        <comment>New SCLK value sub-seconds</comment>
      </arg>
    </args>
  </event>
  <event id="1" name="SphinxTime_TimeSince_CDH_Boot" severity="ACTIVITY_LO" format_string="Elapsed time since Sphinx (CDH) boot-up/power reset = %u.%06u">
    <comment>
     Time since Sphinx boot-up/power reset 
    </comment>
    <args>
      <arg name="secs" type="U32">
        <comment>Up-time whole seconds</comment>
      </arg>
      <arg name="usecs" type="U32">
        <comment>Up-time sub-seconds</comment>
      </arg>
    </args>
  </event>
</events>    
